{
  "workRoot": "_AudioSorter",
  "extensions": [
    "mp3",
    "flac",
    "wav",
    "m4a",
    "aac",
    "ogg"
  ],
  "destinationTemplate": "{bpmRange}",
  "genre": {
    "unknownName": "Unknown",
    "sanitizeForFolder": true
  },
  "bpm": {
    "useSubfolders": true,
    "bucketSize": 10,
    "noBpmFolderName": "No-BPM"
  },
  "musicBrainz": {
    "delayMs": 900,
    "userAgent": "AudioSorter/1.0 (+https://github.com/yourname/AudioSorter)"
  }
}